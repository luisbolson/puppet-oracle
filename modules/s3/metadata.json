{
  "name": "malnick-s3",
  "version": "0.1.0",
  "author": "Jeff Malnick",
  "summary": "This module uses unicorns to fetch files from s3.",
  "license": "Apache-2.0",
  "source": "https://github.com/malnick/puppet-s3",
  "project_page": "https://github.com/malnick/puppet-s3",
  "issues_url": "https://github.com/malnick/puppet-s3/issues",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">3.2.0 <4.0.0"
    },
    {
